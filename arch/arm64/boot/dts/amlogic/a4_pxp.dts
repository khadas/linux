/dts-v1/;

#include "meson-a4.dtsi"
#include <dt-bindings/input/input.h>

/ {
	model = "Amlogic";
	amlogic-dt-id = "a4_pxp";
	compatible = "amlogic, a4";
	interrupt-parent = <&gic>;
	#address-cells = <2>;
	#size-cells = <2>;

	aliases {
		serial0 = &uart_B;
		serial1 = &uart_A;
		serial2 = &uart_D;
		serial3 = &uart_E;
		//i2c0 = &i2c0;
		//i2c1 = &i2c1;
		//i2c2 = &i2c2;
		//i2c3 = &i2c3;
		//spi0 = &spifc;
		spi1 = &spicc0;
		spi2 = &spicc1;
		//tsensor0 = &p_tsensor;
	};

	memory@00000000 {
		device_type = "memory";
		linux,usable-memory = <0x0 0x0 0x0 0x40000000>;
	};

	reserved-memory {
		#address-cells = <2>;
		#size-cells = <2>;
		ranges;

		ramdump_bl33z@00000000 {
			reg = <0x0 0x0 0x0 0x1800000>; /* 0 ~ 24M */
			status = "disabled";
		};

		dsp_fw_reserved:linux,dsp_fw {
			compatible = "shared-dma-pool";
			reusable;
			size = <0x0 0x00800000>;
			alignment = <0x0 0x00400000>;
			alloc-ranges = <0x0 0x30000000 0x0 0x00800000>;
		};

		/* global autoconfigured region for contiguous allocations */
		ramoops@0x07400000 {
			compatible = "ramoops";
			reg = <0x0 0x07400000 0x0 0x00100000>;
			record-size = <0x20000>;
			console-size = <0x40000>;
			ftrace-size = <0x80000>;
			pmsg-size = <0x20000>;
		};

		secmon_reserved:linux,secmon {
			compatible = "shared-dma-pool";
			reusable;
			size = <0x0 0x3400000>;
			alignment = <0x0 0x400000>;
			alloc-ranges = <0x0 0x05000000 0x0 0x3400000>;
		};

		logo_reserved:linux,meson-fb {
			compatible = "shared-dma-pool";
			reusable;
			/* keep sync with fb */
			/* fox example:logo_addr = "0x3f800000"*/
			size = <0xc00000>;
			alignment = <0x400000>;
			reg = <0x0 0x3f000000 0x0 0xc00000>;
		};

		ion_cma_reserved:linux,ion-dev {
			compatible = "shared-dma-pool";
			reusable;
			size = <0x2000000>;
			alignment = <0x400000>;
		};
	};

	vcc5v_reg: fixed@vcc5v_reg{
		vin-supply = <&dc_12v>;
		compatible = "regulator-fixed";
		regulator-name = "VCC5V";
		regulator-min-microvolt = <5000000>;
		regulator-max-microvolt = <5000000>;
		regulator-boot-on;
	};

	vddio3v3_reg: fixed@vddio3v3_reg{
		vin-supply = <&vddao3v3_reg>;
		compatible = "regulator-fixed";
		regulator-name = "VDDIO_3V3";
		regulator-min-microvolt = <3300000>;
		regulator-max-microvolt = <3300000>;
		gpio = <&ao_gpio GPIO_TEST_N GPIO_ACTIVE_HIGH>;
		enable-active-high;
		regulator-boot-on;
		regulator-always-on;
	};

	vddio_x: gpio-regulator@vddio_x {
		vin-supply = <&vddao3v3_reg>;
		compatible = "regulator-gpio";
		regulator-name = "VDDIO_X";
		regulator-min-microvolt = <1800000>;
		regulator-max-microvolt = <3300000>;
		gpios = <&gpio GPIOD_8 GPIO_ACTIVE_LOW>;
		gpios-states = <1>;
		states = <1800000 1
			  3300000 0>;
		regulator-boot-on;
	};

	sd_3v3_reg: fixed@sd_3v3_reg{
		vin-supply = <&vddao3v3_reg>;
		compatible = "regulator-fixed";
		regulator-name = "SD_3V3";
		regulator-min-microvolt = <3300000>;
		regulator-max-microvolt = <3300000>;
		regulator-boot-on;
		regulator-always-on;
	};

	vddds1v8_reg: fixed@vddds1v8_reg{
		vin-supply = <&vddao3v3_reg>;
		compatible = "regulator-fixed";
		regulator-name = "VDD_DSOFF_1V8";
		regulator-min-microvolt = <1800000>;
		regulator-max-microvolt = <1800000>;
		gpio = <&ao_gpio GPIOAO_0 GPIO_ACTIVE_HIGH>;
		enable-active-high;
		regulator-boot-on;
		regulator-always-on;
	};

	provisionkey {
		compatible = "amlogic, provisionkey";
		status = "disabled";
		key-permit-default = "write";
		//new key not need add dts if started with KEY_PROVISION_
		KEY_PROVISION_XXX { };
		//test_my_added_keyname { };
	};//End provisionkey

	unifykey{
		compatible = "amlogic,unifykey";
		status = "okay";
		unifykey-num = <8>;
		unifykey-index-0 = <&keysn_0>;
		unifykey-index-1 = <&keysn_1>;
		unifykey-index-2 = <&keysn_2>;
		unifykey-index-3 = <&keysn_3>;
		unifykey-index-4 = <&keysn_4>;
		unifykey-index-5 = <&keysn_5>;
		unifykey-index-6 = <&keysn_6>;
		unifykey-index-7 = <&keysn_7>;

		keysn_0:key_0{
			key-name = "usid";
			key-device = "normal";
			key-permit = "read","write","del";
		};
		keysn_1:key_1{
			key-name = "mac";
			key-device = "normal";
			key-permit = "read","write","del";
		};
		keysn_2:key_2{
			key-name = "secure_boot_set";
			key-device = "efuse";
			key-permit = "write";
		};
		keysn_3:key_3{
			key-name = "mac_bt";
			key-device = "normal";
			key-permit = "read","write","del";
			key-type  = "mac";
		};
		keysn_4:key_4{
			key-name = "mac_wifi";
			key-device = "normal";
			key-permit = "read","write","del";
			key-type = "mac";
		};
		keysn_5:key_5{
			key-name = "deviceid";
			key-device = "normal";
			key-permit = "read","write","del";
		};
		keysn_6:key_6{
			key-name = "region_code";
			key-device = "normal";
			key-permit = "read","write","del";
		};
		keysn_7:key_7{
			key-name = "oemkey";
			key-device = "normal";
			key-permit = "read","write","del";
		};
	};//End unifykey

	efusekey:efusekey{
		keynum = <4>;
		key0 = <&key_0>;
		key1 = <&key_1>;
		key2 = <&key_2>;
		key3 = <&key_3>;
		key_0:key_0{
			keyname = "mac";
			offset = <0>;
			size = <6>;
		};
		key_1:key_1{
			keyname = "mac_bt";
			offset = <6>;
			size = <6>;
		};
		key_2:key_2{
			keyname = "mac_wifi";
			offset = <12>;
			size = <6>;
		};
		key_3:key_3{
			keyname = "usid";
			offset = <18>;
			size = <16>;
		};
	};//End efusekey
	/* Audio Related start */
	auge_sound {
		compatible = "amlogic, auge-sound-card";
		aml-audio-card,name = "AML-AUGESOUND";

		/*avout mute gpio*/
		//avout_mute-gpios = <&gpio GPIOH_5 GPIO_ACTIVE_HIGH>;
		//spk_mute-gpios = <&gpio GPIOD_2 GPIO_ACTIVE_LOW>;

		aml-audio-card,dai-link@0 {
			format = "i2s";
			mclk-fs = <256>;
			//continuous-clock;
			//bitclock-inversion;
			//frame-inversion;
			/* master mode */
			bitclock-master = <&tdma>;
			frame-master = <&tdma>;
			/* slave mode */
			/*
			 * bitclock-master = <&tdmacodec>;
			 * frame-master = <&tdmacodec>;
			 */
			suffix-name = "alsaPORT-pcm";
			tdmacpu: cpu {
				sound-dai = <&tdma>;
				dai-tdm-slot-tx-mask =
							<1 1>;
				dai-tdm-slot-rx-mask =
							<1 1>;
				dai-tdm-slot-num = <2>;
				dai-tdm-slot-width = <32>;
				system-clock-frequency = <12288000>;
			};
			tdmacodec: codec {
				sound-dai = <&dummy_codec>;
			};
		};

		aml-audio-card,dai-link@1 {
			format = "i2s";// "dsp_a";
			mclk-fs = <256>;
			//continuous-clock;
			//bitclock-inversion;
			//frame-inversion;
			/* master mode */
			bitclock-master = <&tdmb>;
			frame-master = <&tdmb>;
			/* slave mode */
			//bitclock-master = <&tdmbcodec>;
			//frame-master = <&tdmbcodec>;
			/* suffix-name, sync with android audio hal
			 * what's the dai link used for
			 */
			suffix-name = "alsaPORT-i2s";
			cpu {
				sound-dai = <&tdmb>;
				dai-tdm-slot-tx-mask = <1 1>;
				//dai-tdm-slot-rx-mask = <1 1>;
				dai-tdm-slot-num = <2>;
				/*
				 * dai-tdm-slot-tx-mask =
				 *	<1 1 1 1 1 1 1 1>;
				 * dai-tdm-slot-rx-mask =
				 *	<1 1 1 1 1 1 1 1>;
				 * dai-tdm-slot-num = <8>;
				 */
				dai-tdm-slot-width = <32>;
				system-clock-frequency = <12288000>;
			};
			tdmbcodec: codec {
				sound-dai = <&dummy_codec /* &amlogic_codec */ /*&ad82584f_62*/>;
			};
		};

		aml-audio-card,dai-link@2 {
			mclk-fs = <64>;
			/* suffix-name, sync with android audio hal
			 * what's the dai link used for
			 */
			suffix-name = "alsaPORT-pdm";
			cpu {
				sound-dai = <&apdm>;
			};
			codec {
				sound-dai = <&dummy_codec>;
			};
		};

		aml-audio-card,dai-link@3 {
			mclk-fs = <128>;
			/* suffix-name, sync with android audio hal used for */
			suffix-name = "alsaPORT-spdif";
			cpu {
				sound-dai = <&spdifa>;
				system-clock-frequency = <6144000>;
			};
			codec {
				sound-dai = <&dummy_codec>;
			};
		};

		aml-audio-card,dai-link@4 {
			mclk-fs = <256>;
			continuous-clock;
			suffix-name = "alsaPORT-loopback";
			cpu {
				sound-dai = <&loopbacka>;
				system-clock-frequency = <12288000>;
			};
			codec {
				sound-dai = <&dummy_codec>;
			};
		};

		aml-audio-card,dai-link@5 {
			format = "i2s";
			mclk-fs = <256>;
			//continuous-clock;
			//bitclock-inversion;
			//frame-inversion;
			/* master mode */
			bitclock-master = <&tdmc>;
			frame-master = <&tdmc>;
			/* slave mode */
			//bitclock-master = <&tdmccodec>;
			//frame-master = <&tdmccodec>;
			/* suffix-name, sync with android audio hal used for */
			suffix-name = "alsaPORT-tdmin";
			cpu {
				sound-dai = <&tdmc>;
				//dai-tdm-slot-tx-mask = <1 1>;
				dai-tdm-slot-rx-mask = <1 1>;
				dai-tdm-slot-num = <2>;
				dai-tdm-slot-width = <32>;
				system-clock-frequency = <12288000>;
			};
			codec {
				sound-dai = <&dummy_codec>;
			};
		};
	};

	audiolocker: locker {
		compatible = "amlogic, audiolocker";
		clock-names = "lock_out", "lock_in", "out_src",
					"in_src", "out_calc", "in_ref";
		interrupts = <GIC_SPI 1 IRQ_TYPE_EDGE_RISING>;
		interrupt-names = "irq";
		frequency = <49000000>; /* pll */
		dividor = <49>; /* locker's parent */
		status = "disabled";
	};/* Audio Related end */

	adc_keypad {
		compatible = "amlogic, adc_keypad";
		status = "okay";
		key_name = "update", "vol-", "vol+", "ok" ,"power";
		key_num = <5>;
		io-channels = <&saradc 2>;
		io-channel-names = "key-chan-2";
		key_chan = <2 2 2 2 2>;
		key_code = <KEY_SETUP KEY_VOLUMEDOWN KEY_VOLUMEUP KEY_ENTER KEY_POWER>;
		key_val = <0 143 266 389 512>; //val=voltage/1800mV*1023
		key_tolerance = <40 40 40 40 40>;
	};

	gpio_keypad{
		compatible = "amlogic, gpio_keypad";
		status = "disabled";
		scan_period = <20>;
		key_num = <1>;
		key_name = "mute";
		key_code = <SW_MUTE_DEVICE>;
		key_type = <EV_SW>;
		key-gpios = <&gpio  GPIOD_3  GPIO_ACTIVE_HIGH>;
		detect_mode = <0>;/*0:polling mode, 1:irq mode*/
	};

	partitions: partitions{
		parts = <5>;
		part-0 = <&recovery>;
		part-1 = <&misc>;
		part-2 = <&boot>;
		part-3 = <&system>;
		part-4 = <&data>;

		recovery:recovery{
			pname = "recovery";
			size = <0x0 0x2000000>;
			mask = <1>;
		};
		misc:misc{
			pname = "misc";
			size = <0x0 0x800000>;
			mask = <1>;
		};
		boot:boot
		{
			pname = "boot";
			size = <0x0 0x4000000>;
			mask = <1>;
		};
		system:system
		{
			pname = "system";
			size = <0x0 0x40000000>;
			mask = <1>;
		};
		data:data
		{
			pname = "data";
			size = <0xffffffff 0xffffffff>;
			mask = <4>;
		};
	};
};

&audiobus {
	tdma: tdm@0 {
		compatible = "amlogic, a5-snd-tdma";
		#sound-dai-cells = <0>;
		dai-tdm-lane-slot-mask-in = <1 1 1 1>;
		dai-tdm-lane-slot-mask-out = <1 1>;
		dai-tdm-clk-sel = <0>;
		clocks = <&clkaudio CLKID_AUDIO_MCLK_A
				&clkaudio CLKID_AUDIO_MCLK_PAD2
				&clkc CLKID_HIFI_PLL>;
		clock-names = "mclk", "mclk_pad", "clk_srcpll";
		pinctrl-names = "tdm_pins", "tdmout_a_gpio";
		pinctrl-0 = <
			&mclk_2_pins
			&tdmain_pins
			&tdm_d8_pins
			&tdm_d9_pins
			&tdm_d10_pins
			&tdm_d11_pins
			&tdmain_clk_pins
			>;
		pinctrl-1 = <&tdmin_a_gpio>;
		tdm5v-supply = <&vcc5v_reg>;
		tdm3v3-supply = <&vddio3v3_reg>;
		status = "okay";
	};

	tdmb: tdm@1 {
		compatible = "amlogic, a5-snd-tdmb";
		#sound-dai-cells = <0>;
		dai-tdm-lane-slot-mask-out = <1 1 1 1 1 1 1 1>;
		dai-tdm-clk-sel = <1>;
		clocks = <&clkaudio CLKID_AUDIO_MCLK_B
				&clkaudio CLKID_AUDIO_MCLK_PAD1
				&clkc CLKID_HIFI_PLL>;
		clock-names = "mclk", "mclk_pad", "clk_srcpll";
		pinctrl-names = "tdm_pins", "tdmout_a_gpio";
		pinctrl-0 = <
			&mclk_1_pins
			&tdm_b_pins
			&tdm_d2_pins
			&tdm_d3_pins
			&tdm_d4_pins
			&tdm_d5_pins
			&tdm_d6_pins
			&tdm_d7_pins
			&tdm_d12_pins
			&tdm_d13_pins
			&tdmb_clk_pins>;
		pinctrl-1 = <&tdmout_b_gpio>;
		start_clk_enable = <1>;
		tdm5v-supply = <&vcc5v_reg>;
		tdm3v3-supply = <&vddio3v3_reg>;
		status = "okay";
	};

	tdmc:tdm@2 {
		compatible = "amlogic, a5-snd-tdmc";
		#sound-dai-cells = <0>;
		dai-tdm-lane-slot-mask-in = <1 1>;
		dai-tdm-clk-sel = <2>;
		clocks = <&clkaudio CLKID_AUDIO2_MCLK_VAD
				&clkc CLKID_HIFI_PLL>;
		clock-names = "mclk","clk_srcpll";
		//pinctrl-names = "tdm_pins";
		//pinctrl-0 = <
			//&mclk_2_pins
			//&tdm_c_pins
			//&tdm_d11_pins
			//&tdmc_clk_pins>;

		tdm5v-supply = <&vcc5v_reg>;
		tdm3v3-supply = <&vddio3v3_reg>;
		status = "okay";
	};

	spdifa: spdif@0 {
		compatible = "amlogic, a5-snd-spdif-a";
		#sound-dai-cells = <0>;
		clocks = <&clkc CLKID_HIFI_PLL
				&clkc CLKID_FCLK_DIV4
				&clkaudio CLKID_AUDIO_GATE_SPDIFIN
				&clkaudio CLKID_AUDIO_GATE_SPDIFOUT_A
				&clkaudio CLKID_AUDIO_SPDIFIN
				&clkaudio CLKID_AUDIO_SPDIFOUT_A>;
		clock-names = "sysclk", "fixed_clk", "gate_spdifin",
				"gate_spdifout", "clk_spdifin", "clk_spdifout";
		interrupts =
				<GIC_SPI 35 IRQ_TYPE_EDGE_RISING>;

		interrupt-names = "irq_spdifin";
		//pinctrl-names = "spdif_pins", "spdif_pins_mute";
		//pinctrl-0 = <&spdifout
		//	     &spdifin>;
		//pinctrl-1 = <&spdif_a_mute>;

		/*spdif clk tuning enable*/
		clk_tuning_enable = <1>;

		spdif5v-supply = <&vcc5v_reg>;
		spdif3v3-supply = <&vddio3v3_reg>;
		status = "okay";
	};

	apdm: pdm {
		compatible = "amlogic, a5-snd-pdm";
		#sound-dai-cells = <0>;

		clocks = <&clkaudio CLKID_AUDIO2_GATE_PDM
			&clkc CLKID_FCLK_DIV4
			&clkc CLKID_HIFI_PLL
			&clkaudio CLKID_AUDIO2_PDM_DCLK
			&clkaudio CLKID_AUDIO2_PDM_SYSCLK>;
		clock-names = "gate",
			"sysclk_srcpll",
			"dclk_srcpll",
			"pdm_dclk",
			"pdm_sysclk";
		//power-domains = <&pwrdm PDID_A5_AUDIO_PDM>;
		//pinctrl-names = "pdm_pins";
		//pinctrl-0 = <&pdmin>;

		/* mode 0~4, defalut:1 */
		filter_mode = <1>;

		pdm5v-supply = <&vcc5v_reg>;
		pdm3v3-supply = <&vddio3v3_reg>;
		status = "okay";
	};

	/* a5 only one resample, a/b can only use one at the same time */
	/* spdif use this resample */
	asrca: resample@0 {
		compatible = "amlogic, t5-resample-a";
		//clocks = <&clkc CLKID_MPLL2
		//		&clkaudio CLKID_AUDIO_MCLK_F
		//		&clkaudio CLKID_AUDIO_RESAMPLE_A>;
		//clock-names = "resample_pll", "resample_src", "resample_clk";
		/*same with toddr_src
		 *	TDMIN_A, 0
		 *	TDMIN_B, 1
		 *	TDMIN_C, 2
		 *	SPDIFIN, 3
		 *	PDMIN,  4
		 *	NONE,
		 *	TDMIN_LB, 6
		 *	LOOPBACK, 7
		 */
		resample_module = <3>;
		status = "disabled";
	};

	/* loopback use this resample */
	asrcb: resample@1 {
		compatible = "amlogic, t5-resample-b";
		//clocks = <&clkc CLKID_MPLL2
		//		&clkaudio CLKID_AUDIO_MCLK_F
		//		&clkaudio CLKID_AUDIO_RESAMPLE_A>;
		//clock-names = "resample_pll", "resample_src", "resample_clk";
		/*this resample is only used for loopback_A.*/
		capture_sample_rate = <16000>;

		status = "disabled";
	};

	vad:vad {
		compatible = "amlogic, snd-vad-top";
		#sound-dai-cells = <0>;

		clocks = <&clkaudio CLKID_AUDIO2_GATE_TOVAD
			&clkc CLKID_FCLK_DIV4
			&clkaudio CLKID_AUDIO2_VAD_CLK>;
		clock-names = "gate", "pll", "clk";

		interrupts = <GIC_SPI 39 IRQ_TYPE_EDGE_RISING
				GIC_SPI 44 IRQ_TYPE_EDGE_RISING>;
		interrupt-names = "irq_wakeup", "irq_frame_sync";

		/*
		 * Data src sel:
		 * 0: tdmin_a;
		 * 1: tdmin_b;
		 * 2: tdmin_c;
		 * 3: spdifin;
		 * 4: pdmin;
		 * 5: loopback_b;
		 * 6: tdmin_lb;
		 * 7: loopback_a;
		 */
		src = <4>;

		/*
		 * deal with hot word in user space or kernel space
		 * 0: in user space
		 * 1: in kernel space
		 */
		level = <1>;

		status = "okay";
	};

	loopbacka:loopback@0 {
		compatible = "amlogic, a5-loopbacka";
		#sound-dai-cells = <0>;

		clocks = <&clkaudio CLKID_AUDIO2_GATE_PDM
			&clkc CLKID_FCLK_DIV4
			&clkc CLKID_HIFI_PLL
			&clkaudio CLKID_AUDIO2_PDM_DCLK
			&clkaudio CLKID_AUDIO2_PDM_SYSCLK
			&clkc CLKID_HIFI_PLL
			&clkaudio CLKID_AUDIO_MCLK_B>;
		clock-names = "pdm_gate",
			"pdm_sysclk_srcpll",
			"pdm_dclk_srcpll",
			"pdm_dclk",
			"pdm_sysclk",
			"tdminlb_mpll",
			"tdminlb_mclk";

		/* datain src
		 * 0: tdmin_a;
		 * 1: tdmin_b;
		 * 2: tdmin_c;
		 * 3: spdifin;
		 * 4: pdmin;
		 */
		datain_src = <4>;
		datain_chnum = <4>;
		datain_chmask = <0xf>;
		/* config which data pin for loopback */
		datain-lane-mask-in = <1 0 1 0>;

		/* calc mclk for datalb */
		mclk-fs = <256>;

		/* tdmin_lb src
		 * 0: tdmoutA
		 * 1: tdmoutB
		 * 2: tdmoutC
		 * 3: PAD_TDMINA_DIN*, refer to core pinmux
		 * 4: PAD_TDMINB_DIN*, refer to core pinmux
		 * 5: PAD_TDMINC_DIN*, refer to core pinmux
		 * 6: PAD_TDMINA_D*, oe, refer to core pinmux
		 * 7: PAD_TDMINB_D*, oe, refer to core pinmux
		 */
		/* if tdmin_lb >= 3, use external loopback */
		datalb_src = <1>;
		datalb_chnum = <2>;
		datalb_chmask = <0x3>;
		/* config which data pin as loopback */
		datalb-lane-mask-in = <1 0 0 0>;

		status = "okay";
	};

	aed:effect {
		compatible = "amlogic, snd-effect-v4";
		#sound-dai-cells = <0>;
		//clocks = <&clkaudio CLKID_AUDIO_GATE_EQDRC
		//	&clkc CLKID_FCLK_DIV4
		//	&clkaudio CLKID_AUDIO_EQDRC>;
		//clock-names = "gate", "srcpll", "eqdrc";
		/*
		 * 0:tdmout_a
		 * 1:tdmout_b
		 * 2:tdmout_c
		 * 3:spdifout
		 * 4:spdifout_b
		 */
		eqdrc_module = <1>;
		/* max 0xf, each bit for one lane, usually one lane */
		lane_mask = <0x1>;
		/* max 0xff, each bit for one channel */
		channel_mask = <0xff>;
		status = "disabled";
	};
}; /* end of audiobus */

&periphs_pinctrl {

	tdmain_pins: tdmain_pin {
		mux { /* GPIOT_1, GPIOT_0, GPIOT_2, GPIOT_3, GPIOT_4, GPIOT_5 */
			groups = "tdm_sclk2_t",
				"tdm_fs2_t",
				"tdm_d8_t",
				"tdm_d9_t",
				"tdm_d10_t",
				"tdm_d11_t";
			function = "tdm";
		};
	};

	tdm_b_pins: tdm_b_pin {
		mux { /* GPIOC_0, GPIOC_1, GPIOC_2--GPIOC_8, GPIOT7, GPIOT8*/
			groups = "tdm_d2_c",
				"tdm_d3_c",
				"tdm_fs1_c",
				"tdm_sclk1_c",
				"tdm_d4_c",
				"tdm_d5_c",
				"tdm_d6_c",
				"tdm_d7_c",
				"tdm_d12_t",
				"tdm_d13_t";
			function = "tdm";
		};
	};

	tdmin_a_gpio: tdmin_a_gpio {
		mux {
			groups ="GPIOT_0",
				"GPIOT_1",
				"GPIOT_2",
				"GPIOT_3",
				"GPIOT_4",
				"GPIOT_5",
				"GPIOT_6";
			function = "gpio_periphs";
			output-low;
		};
	};

	tdmout_b_gpio: tdmout_b_gpio {
		mux {
			groups ="GPIOC_0",
				"GPIOC_1",
				"GPIOC_2",
				"GPIOC_3",
				"GPIOC_4",
				"GPIOC_5",
				"GPIOC_6",
				"GPIOC_7",
				"GPIOC_8",
				"GPIOT_7",
				"GPIOT_8";
			function = "gpio_periphs";
			output-low;
		};
	};
	tdmin_c_gpio: tdmin_c_gpio {
		mux {
			groups ="GPIOX_8",
				"GPIOX_9",
				"GPIOX_10",
				"GPIOX_11";
			function = "gpio_periphs";
			output-low;
		};
	};
	tdmcin_pins: tdmcin_pin {
		mux { /* GPIOX_8, GPIOX_9, GPIOX_10, GPIOX_11, */
			groups = "tdm_sclk0_x",
				"tdm_fs0_x",
				"tdm_d0_x",
				"tdm_d1_x";
			function = "tdm";
		};
	};

	pdmin: pdmin {
		mux { /* GPIOH_2, GPIOH_1, GPIOH_0, GPIOH_4, GPIOH_3 */
			groups = "pdm_dclk_h",
				"pdm_din0_h",
				"pdm_din1_h",
				"pdm_din2_h",
				"pdm_din3_h";
			function = "pdm";
		};
	};

}; /* end of periphs_pinctrl */

