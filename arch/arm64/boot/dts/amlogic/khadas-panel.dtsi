		/* power delay:(unit in ms) */
		bl-gpios = <&gpio GPIOY_13 GPIO_ACTIVE_HIGH
			&gpio GPIOY_8 GPIO_ACTIVE_HIGH>;
		bl_gpio_names = "GPIOY_13","GPIOY_8";

		backlight_0{
			index = <0>;
			bl_name = "backlight_pwm";
