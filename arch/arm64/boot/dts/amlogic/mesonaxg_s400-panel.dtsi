		mode = "tablet";
		fr_auto_policy = <0>; /* 0=disable, 1=60/50hz, 2=60/50/48hz */
		key_valid = <0>;
		clocks = <&clkc CLKID_CLK81_MIPI_DSI_HOST>, <&clkc CLKID_CLK81_MIPI_DSI_PHY>,
