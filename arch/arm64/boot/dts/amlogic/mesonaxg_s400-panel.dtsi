			 <&clkc CLKID_CLK81_MIPI_BANDGAP>;
		clock-names = "dsi_host_gate", "dsi_phy_gate", "dsi_meas",
			      "mipi_enable_gate", "mipi_bandgap_gate";
		reg = <0x0 0xffd06000 0x0 0x400>, /* dsi_host */
		      <0x0 0xff640000 0x0 0x100>; /* dsi_phy */
		interrupts = <0 61 1>;
		interrupt-names = "vsync";
		pinctrl_version = <2>; /* for uboot */

