	bridge = <&i2c2_rk628>;
};

&rgb_in_vp2 {
	status = "okay";
};

&rgb {
	status = "okay";

	ports {
		port@1 {
			reg = <1>;

			rgb_out_panel: endpoint {
				remote-endpoint = <&panel_in_rgb>;
			};
		};
	};
};

&touch_gpio {
	rockchip,pins = <0 RK_PA4 RK_FUNC_GPIO &pcfg_pull_up>,
			<0 RK_PB0 RK_FUNC_GPIO &pcfg_pull_none>;
};
