			status = "okay";
			type = <0>; /* 0=i2c, 1=spi, 2=mipi */
			i2c_address = <0x20>; /* 7bit i2c address */
			i2c_address2 = <0x74>; /* 7bit i2c address */
			cmd_size = <0xff>;

			init_on = <
				0xc0 2 0x01 0x2b
				0xc0 2 0x02 0x05
				0xc0 2 0x03 0x00
				0xc0 2 0x04 0x00
				0xc0 2 0x05 0x0c
				0xc0 2 0x06 0x04
				0xc0 2 0x07 0x21
				0xc0 2 0x08 0x0f
				0xc0 2 0x09 0x04
				0xc0 2 0x0a 0x00
				0xc0 2 0x0b 0x04
				0xc0 2 0xff 0x00
				0xfd 1 100  /* delay 100ms */

				0xc1 2 0x01 0xca
				0xc1 2 0x02 0x3b
				0xc1 2 0x03 0x33
				0xc1 2 0x04 0x05
				0xc1 2 0x05 0x2c
				0xc1 2 0x06 0xf2
				0xc1 2 0x07 0x9c
				0xc1 2 0x08 0x1b
				0xc1 2 0x09 0x82
				0xc1 2 0x0a 0x3d
				0xc1 2 0x0b 0x20
				0xc1 2 0x0c 0x11
				0xc1 2 0x0d 0xc4
				0xc1 2 0x0e 0x1a
				0xc1 2 0x0f 0x31
				0xc1 2 0x10 0x4c
				0xc1 2 0x11 0x12
				0xc1 2 0x12 0x90
				0xc1 2 0x13 0xf7
				0xc1 2 0x14 0x0c
				0xc1 2 0x15 0x20
				0xc1 2 0x16 0x13
