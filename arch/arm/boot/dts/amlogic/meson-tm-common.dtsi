			clock-names = "pcie_refpll",
						"pcie",
						"pcie_phy",
						"pcie_hcsl";
			/*reset-gpio-type 0:Shared pad(no reset)1:OD pad2:Normal pad*/
			gpio-type = <2>;
			pcie-apb-rst-bit = <30>;
			pcie-phy-rst-bit = <29>;
			pcie-ctrl-a-rst-bit = <28>;
			pwr-ctl = <1>;
			pcie-ctrl-sleep-shift = <20>;
			pcie-hhi-mem-pd-shift = <4>;
			pcie-hhi-mem-pd-mask = <0xf>;
			pcie-ctrl-iso-shift = <20>;
