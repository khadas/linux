		pwrc: power-controller {
			compatible = "amlogic,meson-g12a-pwrc";
			reg = <0xff62fcc0 0x40>;
			reg-names = "dos";
			#power-domain-cells = <1>;
			amlogic,ao-sysctrl = <&rti>;
			amlogic,hhi-sysctrl = <&hhi>;
			vpu,reset = <&vpu_pd_rst>;
			nna,reset = <&nn_pd_rst>;
